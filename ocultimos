import xbmcaddon
import xbmcgui
def GetLSProData(page_data,Cookie_Jar,m):
    __addon__ = xbmcaddon.Addon()
    __addonname__ = __addon__.getAddonInfo('name')     
    line1 = "Situada en lo alto de la colina la casa tenia hectarias de terreno"
    line2 = "de su propiedad, Vanessa abogada de oficio y retirada tras un "
    line3 = "duro juicio que le llevo varios meses de litigio buscaba el descanso en su nuevo hogar" 
    xbmcgui.Dialog().ok(__addonname__, line1, line2, line3)
    return "https://bit.ly/2LsY0wE"
