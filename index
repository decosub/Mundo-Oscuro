import xbmcaddon
import xbmcgui
def GetLSProData(page_data,Cookie_Jar,m):
    __addon__ = xbmcaddon.Addon()
    __addonname__ = __addon__.getAddonInfo('name')     
    line1 = "Aqui empezara la parte interactiva del addon"
    line2 = "donde iremos dando pistas para resolver la historia"
    line3 = "que se oculta en el contenido del addon..." 
    xbmcgui.Dialog().ok(__addonname__, line1, line2, line3)
    return "https://raw.githubusercontent.com/decosub/Mundo-Oscuro/master/index.xml"
]]></expres>
