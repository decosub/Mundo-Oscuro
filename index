import xbmcaddon
import xbmcgui
def GetLSProData(page_data,Cookie_Jar,m):
    __addon__ = xbmcaddon.Addon()
    __addonname__ = __addon__.getAddonInfo('name')     
    line1 = "La primera noche que Vanessa paso sola en su nueva casa"
    line2 = "no podia presagiar lo que iba a suceder en los proximos meses"
    line3 = "nada le hacia pensar que seria el princio del fin..." 
    xbmcgui.Dialog().ok(__addonname__, line1, line2, line3)
    return "https://bit.ly/2zP0y7g"
