import xbmcaddon
import xbmcgui
def GetLSProData(page_data,Cookie_Jar,m):
    __addon__ = xbmcaddon.Addon()
    __addonname__ = __addon__.getAddonInfo('name')     
    line1 = "Lo tuyo no es el Terror"
    line2 = "Te recomiendo que sigas mirando peliculas"
    line3 = "de Walt Disney..." 
    xbmcgui.Dialog().ok(__addonname__, line1, line2, line3)
    return "https://bit.ly/2B1QjN9r"
]]></expres>
